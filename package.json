{
  "name": "@hyperse/unplugin-swc",
  "version": "1.0.0",
  "description": "SWC plugin for Vite and Rollup supporting ESM modules",
  "homepage": "https://github.com/hyperse-io/unplugin-swc/tree/main/#readme",
  "bugs": {
    "url": "https://github.com/hyperse-io/unplugin-swc/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperse-io/unplugin-swc.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "build": "tsup",
    "g:changeset": "changeset",
    "g:cz": "cz",
    "g:release": "yarn build && changeset publish",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "defu": "^6.1.4",
    "get-tsconfig": "^4.10.1",
    "path-exists": "^5.0.0",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@hyperse/eslint-config-hyperse": "^1.4.4",
    "@swc/core": "^1.11.29",
    "@types/node": "^22.15.29",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.28.0",
    "rollup": "^4.41.1",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@swc/core": ">=1.3.42"
  },
  "packageManager": "yarn@4.9.1",
  "publishConfig": {
    "access": "public"
  }
}
